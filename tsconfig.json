{
	"compilerOptions": {
		"target": "ESNext",
		"module": "commonJS",
		"rootDir": "./src/",
		"outDir": "./dist",
		"strict": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"experimentalDecorators": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true,
		"typeRoots": [
			"node_modules/@types"
		],
		"sourceMap": false,
		"baseUrl": "./",
	},
	"files": [
		"src/index.ts"
	],
	"include": [
		"./**/*.ts",
		"test/classes/Pagination.js",
		"src/utils/functions/Collector.ts",
		"src/utils/typings/discord/Collector.js",
	],
	"exclude": [
		"node_modules",
		"dist"
	],
}