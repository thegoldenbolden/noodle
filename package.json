{
	"name": "noodles",
	"version": "2.0.1",
	"description": "A multi-purpose discord bot that loves noodles.",
	"main": "./dist/index.js",
	"scripts": {
		"build": "tsc",
		"start": "node --no-warnings -r dotenv/config  .",
		"dev": "pnpm build && node -r dotenv/config ./dist/index.js"
	},
	"author": {
		"name": "Jacob Bolden",
		"url": "https://jacobbolden.com"
	},
	"license": "MIT",
	"private": true,
	"devDependencies": {
		"@types/lodash": "^4.14.202",
		"@types/node": "^20.11.5",
		"prettier": "^3.2.4",
		"tslib": "^2.6.2",
		"typescript": "^5.3.3"
	},
	"dependencies": {
		"axios": "^1.6.5",
		"canvas": "^2.11.2",
		"date-fns": "^3.2.0",
		"discord.js": "^14.14.1",
		"dotenv": "^16.3.1",
		"kitsu": "^10.1.2",
		"lodash": "^4.17.21",
		"openai": "^4.24.7"
	},
	"prettier": {
		"arrowParens": "always",
		"bracketSpacing": true,
		"endOfLine": "lf",
		"insertPragma": false,
		"printWidth": 80,
		"proseWrap": "never",
		"quoteProps": "as-needed",
		"requirePragma": false,
		"semi": true,
		"singleQuote": false,
		"tabWidth": 1,
		"trailingComma": "es5",
		"useTabs": false,
		"parser": "typescript"
	}
}